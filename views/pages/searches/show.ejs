<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Document</title>
</head>
<body>
  <ul>
  <% books.forEach( (book) => { %>
    <li>
        <div><%= book.title %></div>
        <div><%= book.author %></div>
        <div><%= book.description %></div>
        <div><img src="<%= book.image %>" /></div>
    </li>
  <% }); %>
</ul>
</body>
</html>